Conditional1: WOConditional {
	condition = isDirectAction;
	negate = true;
}

Conditional2: WOConditional {
	condition = isDirectAction;
}

Hyperlink1: WOHyperlink {
	action = action;
	fragmentIdentifier = pushURL;
}

JavaScriptDisabled: WOConditional {
    condition=session.javaScriptEnabled;
    negate=true;
}

JavaScriptEnabled: WOConditional {
    condition=session.javaScriptEnabled;
}
LinkNoJS: WOHyperlink {
	class = ^linkClass;
	action = action;
	target = ^target;
        fragmentIdentifier = ^fragment;
}

SubmitButton1: WOSubmitButton {
	actionClass = ^actionClass;
	directActionName = ^directActionName;
	onsubmit = openWindow;
	value = ^value;
}

SubmitButton2: WOSubmitButton {
	action = action;
	value = ^value;
	onclick = openWindow;
}

