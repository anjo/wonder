<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo class="ERXCheckboxMatrix" wocomponentcontent="true">
		<image path="WOCheckboxMatrix.tiff"/>

		<binding name="list"/>
		<binding name="item"/>
		<binding name="selections"/>
		<binding name="maxColumns"/>
		<binding name="goingVertically" defaults="Boolean"/>
		<binding name="contentCellOtherTagString"/>
		<binding name="sortKey"/>
		<binding name="width"/>
		<binding name="cellAlign"/>
		<binding name="cellVAlign"/>
		<binding name="cellpadding"/>
		<binding name="cellspacing"/>
		<binding name="cellWidth"/>
		<binding name="tableOtherTagString"/>

		<validation message="&apos;list&apos; is a required binding">
			<unbound name="list"/>
		</validation>

		<validation message="&apos;item&apos; is a required binding">
			<unbound name="item"/>
		</validation>
	</wo>
</wodefinitions>