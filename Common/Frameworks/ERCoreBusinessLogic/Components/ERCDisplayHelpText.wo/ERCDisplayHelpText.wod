HelpText : ERXLocalizedString {
	value = helpText.value;
	// key = "value";
	valueWhenEmpty = "";
}

ShowCreate : WOConditional {
	condition = showCreate;
}

ShowEdit : WOConditional {
	condition = showEdit;
}

Create :WOHyperlink {
	action = createHelpText;
}

Edit :WOHyperlink {
	action = editHelpText;
}