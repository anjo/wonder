<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo class="ERPCategoryChart" wocomponentcontent="false">

		<binding name="height"/>
		<binding name="width"/>
		<binding name="items"/>
		<binding name="type"/>
		<binding name="nameKey"/>
		<binding name="valueKey"/>
		<binding name="categoryKey"/>
		<binding name="dataset"/>

		<validation message="Either items, nameKey, categoryKey and valueKey or dataset must be bound">
			<or>
				<and>
					<unbound name="items"/>
					<unbound name="nameKey"/>
					<unbound name="valueKey"/>
					<unbound name="categoryKey"/>
				</and>
				<unbound name="dataset"/>
			</or>
		</validation>
	</wo>
</wodefinitions>