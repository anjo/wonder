Textfield: WOTextField {
	value = ^value;
	id = ^id;
	name = ^name;
	dateformat = dateFormat;
	onclick = onClick;
	class = "AjaxCalendarDateSelect";
	size = ^size;
}

CalendarScript: WOJavaScript {
	scriptFile = "calendar_date_select.js";
	framework = "WO2";
}

FormatterScript: WOJavaScript {
	scriptFile = dateFormatScript;
	frameworks = "WO2";
}

HasFormatterScript: WOConditional {
	condition = dateFormatScript;
}
