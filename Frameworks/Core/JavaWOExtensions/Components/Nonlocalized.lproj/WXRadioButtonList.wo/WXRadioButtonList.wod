Repetition: WORepetition {
	list = ^list;
	item = ^item;
	index = index;
}

Radio: WORadioButton {
	class = ^class;
	id = id;
	name = elementName;
	value = ^item.toString.hashCode;
	disabled = ^disabled;
	selection = selection;
}

Label: WOGenericContainer {
	elementName = "label";
	for = id;
}

DisplayString: WOString {
	value = ^displayString;
}
