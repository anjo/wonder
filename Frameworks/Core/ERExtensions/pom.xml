<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<parent>
		<groupId>wonder.common</groupId>
		<artifactId>frameworks</artifactId>
		<version>4.0.0-SNAPSHOT</version>
		<relativePath>..</relativePath>
	</parent>

	<modelVersion>4.0.0</modelVersion>
	<groupId>wonder.common</groupId>
	<artifactId>ERExtensions</artifactId>
	<version>4.0.0-SNAPSHOT</version>
	<name>ERExtensions</name>
	<packaging>woframework</packaging>

	<dependencies>
		<dependency>
			<groupId>wonder.common</groupId>
			<artifactId>JavaWOExtensions</artifactId>
			<version>4.0.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.apple.webobjects</groupId>
			<artifactId>JavaJDBCAdaptor</artifactId>
		</dependency>
		<dependency>
			<groupId>icu4j</groupId>
			<artifactId>icu4j</artifactId>
		</dependency>
	</dependencies>
	
	<build>
		<sourceDirectory>Sources</sourceDirectory>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <excludes>
                        <exclude>com/webobjects/appserver/_private/WOHttpIO.java</exclude>
                        <exclude>com/webobjects/foundation/NSMutableArray.java</exclude>
                    </excludes>
                </configuration>
             </plugin>
         </plugins>
	</build>

</project>