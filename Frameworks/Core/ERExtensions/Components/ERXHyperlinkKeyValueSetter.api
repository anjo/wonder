<?xml version="1.0" encoding="UTF-8"?>
<wodefinitions>
	<wo class="ERXHyperlinkKeyValueSetter" wocomponentcontent="false">

		<binding name="value"/>
		<binding name="binding"/>
		<binding name="string"/>

		<validation message="'binding' is a required binding">
			<unbound name="binding"/>
		</validation>

		<validation message="'value' is a required binding">
			<unbound name="value"/>
		</validation>

		

		<validation message="'binding' must be bound to a settable value">
			<unsettable name="binding"/>
		</validation>
	</wo>
</wodefinitions>
