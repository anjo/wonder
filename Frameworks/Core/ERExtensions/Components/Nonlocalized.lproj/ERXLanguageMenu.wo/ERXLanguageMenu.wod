LanguagePopup: WOPopUpButton {
	class = ^class;
	dir = ^dir;
	disabled = ^disabled;
	displayString = displayStringForLanguage;
	id = ^id;
	name = ^name;
	item = languageOption;
	list = availableLanguages;
	localizeDisplayStrings = ^localizeDisplayStrings;
	noSelectionString = ^noSelectionString;
	size = ^size;
	selection = ^language;
	style = ^style;
	title = ^title;
}