<?xml version="1.0" encoding="UTF-8"?>
<wodefinitions>
	<wo class="ERXFixedLengthString" wocomponentcontent="false">

		<binding name="value"/>
		<binding defaults="Boolean" name="shouldPadToLength"/>
		<binding defaults="Boolean" name="escapeHTML"/>
		<binding name="length"/>
		<binding name="suffixWhenTrimmed"/>

		<validation message="'value' is a required binding">
			<unbound name="value"/>
		</validation>
	<binding name="valueWhenEmpty"/>
    </wo>
</wodefinitions>
