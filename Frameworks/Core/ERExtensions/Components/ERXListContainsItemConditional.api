<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo class="ERXListContainsItemConditional" wocomponentcontent="true">

		<binding name="list"/>
		<binding name="item"/>
		<binding name="negate" defaults="Boolean"/>

		<validation message="&apos;list&apos; is a required binding">
			<unbound name="list"/>
		</validation>

		<validation message="&apos;item&apos; is a required binding">
			<unbound name="item"/>
		</validation>
	</wo>
</wodefinitions>