Form1: WOForm {
	action = loginCheck;
	multipleSubmit = true;
}

TextField1: WOTextField {
	value = login;
	maxlength = databaseKeyLoginSize;
}

TextField2: WOPasswordField {
	value = password;
	maxlength = databaseKeyPasswordSize;
}

CheckBox1: WOCheckBox {
	checked = saveUserPermissions;
}

SubmitButton1: WOSubmitButton {
	action = loginCheck;
	value = "Login";
}

String1: WOString {
	value = message;
}

