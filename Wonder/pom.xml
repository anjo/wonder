		<module>Frameworks</module>
		<module>Examples</module>
		<module>Applications</module>
	</modules>
	<profiles>
		<profile>
			<id>unknown</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<woClassifier>unknown</woClassifier>
				<!-- Any WebObjects version equals or superior to 5.3 -->
				<woversion>[5.3,)</woversion>
			</properties>
		</profile>
		<profile>
			<id>wo53</id>
			<activation>
				<property>
					<name>womajor</name>
					<value>5.3</value>
				</property>
			</activation>
			<properties>
				<woClassifier>wo53</woClassifier>
				<!-- WebObjects version 5.3.x -->
				<woversion>[5.3,5.4)</woversion>
			</properties>
		</profile>
		<profile>
			<id>wo54</id>
			<activation>
				<property>
					<name>womajor</name>
					<value>5.4</value>
				</property>
			</activation>
			<properties>
				<woClassifier>wo54</woClassifier>
				<!-- WebObjects version 5.4.x -->
				<woversion>[5.4,5.5)</woversion>
			</properties>
		</profile>
	</profiles>
	<build>
		<testSourceDirectory>${project.basedir}/Tests</testSourceDirectory>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.objectstyle.woproject.maven2</groupId>
					<artifactId>maven-wolifecycle-plugin</artifactId>
					<version>2.0.16</version>
				</plugin>
			</plugins>
		</pluginManagement>
		<plugins>
			<plugin>
				<groupId>org.objectstyle.woproject.maven2</groupId>
				<artifactId>maven-wolifecycle-plugin</artifactId>
				<extensions>true</extensions>
				<configuration>
					<skipAppleProvidedFrameworks>true</skipAppleProvidedFrameworks>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
