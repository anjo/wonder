	elementName = "table";
}

EntityName: WOString {
	_unroll = true;
	value = entity.name;
}

Help: ERDDebuggingHelp {
	d2wContext = d2wContext;
}

IfHasSelectedObject: ERXNonNullConditional {
  condition = selectedObject;
}

