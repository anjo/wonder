AboveDisplayPropertyKeys: ERD2WExtraDisplayPropertyKeysComponent {
	extraPropertiesKey = "aboveDisplayPropertyKeys";
	localContext = d2wContext;
}

AttributeName: WOString {
	value = displayNameForRelationshipKey;
}

