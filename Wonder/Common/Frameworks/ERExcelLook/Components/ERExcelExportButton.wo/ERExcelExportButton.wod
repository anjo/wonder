AllowsExcelExportConditional: WOConditional {
	condition = ^allowsExcelExport;
}

HasBatches: WOConditional {
	condition = showExportDisplayed;
}

HasNoBatches: WOConditional {
	condition = showExportDisplayed;
	negate = true;
}

ExcelExportAllHyperlink: WOHyperlink {
	action = excelExportAll;
}

ExcelExportDisplayedHyperlink: WOHyperlink {
	action = excelExportDisplayed;
}

IsTaskEqualList: ERXEqualConditional {
	value1 = ^task;
	value2 = "list";
}