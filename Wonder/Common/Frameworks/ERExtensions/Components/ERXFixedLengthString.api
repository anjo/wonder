<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo class="ERXFixedLengthString" wocomponentcontent="false">

		<binding name="value"/>
		<binding name="shouldPadToLength" defaults="Boolean"/>
		<binding name="length"/>
		<binding name="suffixWhenTrimmed"/>

		<validation message="&apos;value&apos; is a required binding">
			<unbound name="value"/>
		</validation>
	</wo>
</wodefinitions>