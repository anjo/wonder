CheckBox1: WOCheckBox {
	checked = showExceptions;
}

ERWOTestResult: ERXWOTestResult {
	showExceptions = showExceptions;
	errorMessage = errorMessage;
	testResult = testResult;
	test = theTest;
	runTime = runTime;
}

Form1: WOForm {
	multipleSubmit = true;
	action = performTest;
}

PopUpButton1: WOPopUpButton {
	list = allTests;
	selection = theTest;
}

SubmitButton1: WOSubmitButton {
	action = performTest;
	value = "Run";
}

