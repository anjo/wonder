BatchNumber: WOTextField {
    value = currentBatchIndex;
    numberformat = "0";
    size = 3;
	onChange = formTarget;
}

Conditional1: WOConditional {
	condition = displayGroup.hasMultipleBatches;
}

Content: WOComponentContent {
}

DGHasQualifier: ERXNonNullConditional {
	condition = displayGroup.qualifier;
}

EntityName3: ERXPluralString {
	count = objectCount;
	value = ^objectName;
}

EntityNameConditional: WOConditional {
    condition=hasObjectName;
}

Image1: WOImage {
	filename = "s.gif";
	framework = "ERExtensions";
	height = 1;
	width = 500;
}

ItemsPerBatch: WOTextField {
    value = numberOfObjectsPerBatch;
    numberformat = "0";
    size = 3;
    align="center";
	onChange = formTarget;
}

LeftArrow: WOImageButton {
    action = displayGroup.displayPreviousBatch;
    filename = "lft-OSarw.gif";
    border = "0";
//    height = "11";
//    width = "16";
    framework = "JavaWOExtensions";
	onClick = formTarget;
}

NavigationTable : WOGenericContainer {
    elementName="Table";
    width=^width;
    cellpadding=4;
    cellspacing=0;
    bgcolor=^bgcolor;
    border=^border;
};

NextBatchLink: WOHyperlink {
    action = displayGroup.displayNextBatch;
}

PreviousBatchLink: WOHyperlink {
    action = displayGroup.displayPreviousBatch;
}

RightArrow: WOImageButton {
    action = displayGroup.displayNextBatch;
    filename = "rt-OSarw.gif";
    border = "0";
//    height = "11";
//    width = "16";
    framework = "JavaWOExtensions";
    onClick = formTarget;
}

SortComponent: WOSortOrderManyKey {
    keyList=^sortKeyList;
    displayGroup=^displayGroup;
}

SortKeyListConditional: WOConditional {
    condition=hasSortKeyList;
}

String1: WOString {
	value = filteredObjectsCount;
}

TotalBatches: WOString {
    value = displayGroup.batchCount;
}
