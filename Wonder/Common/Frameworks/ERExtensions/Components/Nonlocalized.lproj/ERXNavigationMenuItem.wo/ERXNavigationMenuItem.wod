MeetsDisplayConditions:WOConditional {
	condition=meetsDisplayConditions;
}

NewMarker: WOImage {
	filename = "NewMarker.gif";
	framework = "ERExtensions";
}

NotUsingTablesConditional:WOConditional {
	condition=false;
}

Td: WOGenericContainer { 
    elementName = "td";
    width = navigationItemWidth;
    class = linkClass;
    id = navigationItemID;
    nowrap="nowrap";
}

UsingTablesConditional:WOConditional {
	condition=true;
}

contextComponentActionURL: WOGenericContainer { 
	elementName = "a";
	href = contextComponentActionURL;
	invokeAction = menuItemSelected;
}

displayName: WOString {
	value = displayName;
    escapeHTML=false;
}

hasActivityAndIsEnabled: WOConditional {
	condition = hasActivityAndIsEnabled;
}

hasNoActivityAndIsEnabled: WOConditional {
	condition = hasActivityAndIsEnabled;
	negate = true;
}

li: WOGenericContainer { 
    elementName = "li";
    width = navigationItemWidth;
    class = linkClass;
    id = navigationItemID;
    nowrap="nowrap";
}
