<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo class="ERXOptionalForm" wocomponentcontent="true">

		<binding name="hasForm" defaults="Boolean"/>
		<binding name="action"/>
		<binding name="name"/>
		<binding name="enctype"/>
		<binding name="multipleSubmit" defaults="Boolean"/>

		<validation message="&apos;hasForm&apos; is a required binding">
			<unbound name="hasForm"/>
		</validation>
	</wo>
</wodefinitions>