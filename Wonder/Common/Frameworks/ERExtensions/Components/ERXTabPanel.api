<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo class="ERXTabPanel" wocomponentcontent="true">

		<binding name="tabs"/>
		<binding name="tabNameKey"/>
		<binding name="selectedTab"/>
		<binding name="nonSelectedBgColor"/>
		<binding name="bgcolor"/>
		<binding name="submitActionName"/>
		<binding name="textColor"/>
		<binding name="borderColor"/>
		<binding name="tabImageFramework"/>
		<binding name="leftTabImage"/>
		<binding name="rightTabImage"/>
		<binding name="tabClass"/>
		<binding name="nonSelectedTabClass"/>
		<binding name="tabImageContainerClass"/>
