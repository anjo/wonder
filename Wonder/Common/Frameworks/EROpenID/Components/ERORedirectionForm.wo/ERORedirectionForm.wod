RedirectionForm : WOGenericContainer {
	elementName = "form";
	name = ^formName;
	method = "post"
	action = ^redirectionUrl;
	accept-charset = "utf-8";
}

FormKeyRepetition : WORepetition {
	list = formKeys;
	item = repetitionFormKey;
}

FormHiddenField : WOGenericElement {
	elementName = "input";
	type = "hidden";
	name = repetitionFormKey;
	value = formValue;
}