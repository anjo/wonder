<?xml version="1.0" encoding="UTF-8"?>
<wodefinitions>
	<wo class="ERXImage" wocomponentcontent="false">

		<binding name="height"/>
		<binding name="width"/>
		<binding name="relativePath"/>
		<binding name="host"/>
		<binding name="alt"/>
		<binding name="mimeType"/>

		<validation message="'relativePath' is a required binding">
			<unbound name="relativePath"/>
		</validation>
	<binding name="id"/><binding name="name"/><binding name="cssClass"/></wo>
</wodefinitions>