AjaxExampleComponent : AjaxExampleComponent {
	title = "Ajax Tree";
}

AjaxTree : AjaxTree {
	id = "exampleTree";
	root = rootTreeNode;
	item = treeNode;
	parentKeyPath = "parentTreeNode";
	childrenKeyPath = "childrenTreeNodes";
	rootExpanded = true;
	isLeafKeyPath = "isLeaf";
}

TreeNodeAction : WOHyperlink {
	action = nodeSelected;
	string = treeNode.toString;
}

Busy : AjaxBusyIndicator {
	busyImage = "busyBigSpinner.gif";
	busyImageFramework = "Ajax";
}