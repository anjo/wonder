<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo wocomponentcontent="true" class="AjaxSortableList.java">
		<binding name = "id"/>
		<binding name = "list"/>
		<binding name = "item"/>
		<binding name = "listElementName"/>
		<binding name = "listItemElementName"/>
		<binding name = "listItemIDKeyPath"/>
		<binding name = "startIndex"/>
		<binding name = "action"/>
		
		<binding name = "tag"/>
		<binding name = "only"/>
		<binding name = "overlap"/>
		<binding name = "constraint"/>
		<binding name = "containment"/>
		<binding name = "handle"/>
		<binding name = "hoverclass"/>
		<binding name = "ghosting"/>
		<binding name = "dropOnEmpty"/>
		<binding name = "scroll"/>
		<binding name = "onChange"/>
		<binding name = "onUpdate"/>
	
    <validation message = "&apos;list&apos;, &apos;item&apos;, and &apos;listItemIDKeyPath&apos; are required bindings.">
    	<or>
	      <unbound name = "list"/>
	      <unbound name = "item"/>
  	    <unbound name = "listItemIDKeyPath"/>
  	  </or>
    </validation>
	</wo>
</wodefinitions>