
CheatSheet: ERD2WCheatSheet {
	localContext = d2wContext;
	displayGroup = displayGroup;
}

Entities: WORepetition {
	item = currentEntity;
	list = entities.@sortAsc.name;
}

Tasks: WORepetition {
	item = currentTask;
	list = availableTasks.@sortAsc.toString;
}

SelectEntity: WOHyperlink {
	action = selectEntity;
	string = currentEntity.name;
}

SelectTask: WOHyperlink {
	action = selectTask;
	string = currentTask;
}
